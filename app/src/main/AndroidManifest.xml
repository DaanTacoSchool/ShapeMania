<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="trdaan.shapemania">

    <application
        android:allowBackup="true"
        android:icon="@drawable/launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LevelFinishedActivity"></activity>
        <activity android:name=".GameActivity"></activity>
        <activity android:name=".GameOverActivity"></activity>
        <activity android:name=".PlayerProfileMainActivity"></activity>
        <activity android:name=".SelectLevelActivity"></activity>
        <activity android:name=".GameMenuActivity"></activity>
        <activity android:name=".SettingsActivity"></activity>
        <activity android:name=".AboutActivity"></activity>
        <activity android:name=".NewUserActivity"></activity>
        <activity android:name=".SelectPlayerActivity"></activity>
        <activity android:name=".HighscoreActivity"></activity>

    </application>

</manifest>